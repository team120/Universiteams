{
  "name": "application",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env NODE_ENV=dev nest start",
    "start:dev": "env NODE_ENV=dev nest start --watch",
    "start:debug": "env NODE_ENV=dev nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "env NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env NODE_ENV=test jest",
    "test:watch": "env NODE_ENV=test jest --watch",
    "test:cov": "env NODE_ENV=test jest --coverage",
    "test:debug": "env NODE_ENV=test node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --watchAll --runInBand",
    "test:e2e": "env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:debug": "env NODE_ENV=test node --inspect-brk=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --testNamePattern=\"$npm_config_name\" --runInBand"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/swagger": "^7.0.4",
    "@nestjs/typeorm": "^10.0.0",
    "@sendgrid/mail": "^8.1.0",
    "@sendinblue/client": "^3.1.0",
    "argon2": "^0.28.2",
    "bull": "^4.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^9.0.1",
    "nestjs-pino": "^3.5.0",
    "nodemailer": "^6.9.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.5",
    "@types/bull": "^3.15.8",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.8.6",
    "@types/nodemailer": "^6.4.8",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.0.6",
    "minimist": "^1.2.8",
    "pino-pretty": "^10.0.1",
    "prettier": "^2.3.2",
    "set-cookie-parser": "^2.4.8",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "minimist": "^1.2.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
